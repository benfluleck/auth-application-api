{
  "name": "hotel-booking-application-api",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "clean_start:dev": "yarn migrations && babel-watch src/index.js",
    "start:dev": "babel-watch src/index.js",
    "build": "babel src --out-dir dist",
    "pretest": "rm -rf dev.sqlite3 && NODE_ENV=test knex migrate:latest",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha src/tests/index.test.js --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "server": "node dist/index.js",
    "migrations": "knex migrate:latest",
    "drop-migration": "knex migrate:rollback"
  },
  "dependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
    "@babel/plugin-transform-arrow-functions": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/register": "^7.8.0",
    "@sendgrid/mail": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "connect-redis": "^4.0.0",
    "coveralls": "^3.0.2",
    "dotenv": "^7.0.0",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "ioredis": "^4.3.0",
    "knex": "^0.19.1",
    "lodash.isempty": "^4.4.0",
    "mailgen": "^2.0.7",
    "nyc": "^13.1.0",
    "objection": "^1.4.0",
    "pg": "^7.6.1",
    "sqlite3": "^4.0.4",
    "uuidv4": "^4.0.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "babel-watch": "git+https://github.com/monathur/babel-watch.git",
    "mocha": "^6.1.2"
  }
}
